<!DOCTYPE html>
<html lang="en">
<head>
  <title>List Details</title>
  <link rel="stylesheet" href="css/style.css">
</head>
<body>
  <h1>List: <%= list.name %></h1>

  <% list.imageLinks.forEach(link => { %>
    <img src="<%= link %>" alt="dog" width="200px" style="margin: 10px;" />
  <% }) %>

  <p><a href="/lists">Back to Lists</a> | <a href="/logout">Logout</a></p>
</body>
</html>
